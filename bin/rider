declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/ludwiglauer/Library/Application Support/JetBrains/Toolbox/apps/Rider/ch-0/233.14015.60/Rider.app/Contents/MacOS/rider" $wait --args "${ideargs[@]}"